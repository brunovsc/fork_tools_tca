iseIdentifier":"s:22ComposableArchitecture7ReducerP","text":"Reducer","kind":"typeIdentifier"},{"text":"\n","kind":"text"}],"languages":["swift"]}],"kind":"declarations"},{"parameters":[{"content":[{"inlineContent":[{"type":"text","text":"A writable key path from parent state to a property containing child"},{"type":"text","text":" "},{"type":"text","text":"presentation state."}],"type":"paragraph"}],"name":"toPresentationState"},{"content":[{"type":"paragraph","inlineContent":[{"text":"A case path from parent action to a case containing child actions.","type":"text"}]}],"name":"toPresentationAction"},{"content":[{"inlineContent":[{"text":"A reducer that will be invoked with child actions against presented child","type":"text"},{"text":" ","type":"text"},{"text":"state.","type":"text"}],"type":"paragraph"}],"name":"destination"},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The fileID."}]}],"name":"fileID"},{"content":[{"inlineContent":[{"text":"The filePath.","type":"text"}],"type":"paragraph"}],"name":"filePath"},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The line."}]}],"name":"line"},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The column."}]}],"name":"column"}],"kind":"parameters"},{"content":[{"type":"heading","text":"Return Value","level":2,"anchor":"return-value"},{"type":"paragraph","inlineContent":[{"text":"A reducer that combines the child reducer with the parent reducer.","type":"text"}]}],"kind":"content"},{"content":[{"anchor":"discussion","level":2,"text":"Discussion","type":"heading"},{"inlineContent":[{"text":"This version of ","type":"text"},{"code":"ifLet","type":"codeVoice"},{"text":" requires the usage of the ","type":"text"},{"isActive":true,"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Presents()","type":"reference"},{"text":" macro and","type":"text"},{"text":" ","type":"text"},{"isActive":true,"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/PresentationAction","type":"reference"},{"text":" type in your feature’s domain.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"For example, if a parent feature holds onto a piece of optional child state, then it can","type":"text"},{"text":" ","type":"text"},{"text":"perform its core logic ","type":"text"},{"type":"emphasis","inlineContent":[{"text":"and","type":"text"}]},{"text":" the child’s logic by using the ","type":"text"},{"type":"codeVoice","code":"ifLet"},{"text":" operator:","type":"text"}],"type":"paragraph"},{"code":["@Reducer","struct Parent {","  @ObservableState","  struct State {","    @Presents var child: Child.State?","    \/\/ ...","  }","  enum Action {","    case child(PresentationAction<Child.Action>)","    \/\/ ...","  }","","  var body: some ReducerOf<Self> {","    Reduce { state, action in","      \/\/ Core logic for parent feature","    }","    .ifLet(\\.$child, action: \\.child) {","      Child()","    }","  }","}"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"type":"text","text":"The "},{"type":"codeVoice","code":"ifLet"},{"type":"text","text":" operator does a number of things to make integrating parent and child features"},{"type":"text","text":" "},{"type":"text","text":"ergonomic and enforce correctness:"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"text":"It forces a specific order of operations for the child and parent features:","type":"text"}]},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"text":"When a ","type":"text"},{"isActive":true,"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/PresentationAction\/dismiss","type":"reference"},{"text":" action is sent, it runs the parent feature","type":"text"},{"text":" ","type":"text"},{"text":"before the child state is ","type":"text"},{"type":"codeVoice","code":"nil"},{"text":"’d out. This gives the parent feature an opportunity to","type":"text"},{"text":" ","type":"text"},{"text":"inspect the child state one last time before the state is cleared.","type":"text"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"When a "},{"type":"reference","isActive":true,"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/PresentationAction\/presented(_:)"},{"type":"text","text":" action is sent it runs the"},{"type":"text","text":" "},{"type":"text","text":"child first, and then the parent. If the order was reversed, then it would be possible"},{"type":"text","text":" "},{"type":"text","text":"for the parent feature to "},{"type":"codeVoice","code":"nil"},{"type":"text","text":" out the child state, in which case the child feature"},{"type":"text","text":" "},{"type":"text","text":"would not be able to react to that action. That can cause subtle bugs."}]}]}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"It automatically cancels all child effects when it detects the child’s state is "},{"type":"codeVoice","code":"nil"},{"type":"text","text":"’d"},{"type":"text","text":" "},{"type":"text","text":"out."}]}]},{"content":[{"inlineContent":[{"type":"text","text":"Automatically "},{"code":"nil","type":"codeVoice"},{"type":"text","text":"s out child state when an action is sent for alerts and confirmation"},{"type":"text","text":" "},{"type":"text","text":"dialogs."}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"It gives the child feature access to the "},{"type":"reference","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/DismissEffect","isActive":true},{"type":"text","text":" dependency, which allows the"},{"type":"text","text":" "},{"type":"text","text":"child feature to dismiss itself without communicating with the parent."}]}]}],"type":"unorderedList"}],"kind":"content"}],"variants":[{"paths":["\/d